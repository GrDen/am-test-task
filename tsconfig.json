{
  "compilerOptions": {
    "target": "ESNEXT",     
    "module": "commonjs",                           
    "jsx": "react",                           
    "strict": true,                                 
    "baseUrl": "./",                             
    "paths": {
      "components/*": ["src/components/*"],
      "pages/*": ["src/pages/*"],
      "layouts/*": ["src/layouts/*"],
      "helpers/*": ["src/helpers/*"],
      "routes/*": ["src/routes/*"],
      "store/*": ["src/store/*"],
      "store": ["src/store"],
      "models/*": ["src/models/*"],
    },                                 
    "esModuleInterop": true,                        
    "skipLibCheck": true,                           
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true
  }
}
